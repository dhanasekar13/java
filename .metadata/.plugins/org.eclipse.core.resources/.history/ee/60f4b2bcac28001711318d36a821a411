package multithreading;
class a extends Thread
{
	public void run()
	{
		System.out.println(Thread.currentThread());
	}
}
public class threadPriorty {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
a t1=new a();
t1.setName("Thread 1");
t1.setPriority(10);
t1.start();
a t2=new a();
t2.setName("Thread 2");
t2.setPriority(0);

t2.start();
	}

}
