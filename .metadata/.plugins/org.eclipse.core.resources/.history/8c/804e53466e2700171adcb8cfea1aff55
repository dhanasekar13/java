	}
class db2
{ Scanner na=new Scanner(System.in);
static Vector name=new Vector();
	 db2()
	{
		System.out.println("Enter the Name");
			String str1=na.nextLine();
			name.add(str1);		
     }
	 db2(int b)
	 {
		 System.out.println(name.get(b));
	 }
	db2(String a)
		{
		System.out.println("Whose details to view");
		String s=na.nextLine();
		int v=name.indexOf(s);	
		new db1(v);
		new db2(v);
		new db3(v);
		new db4(v);
		new db5(v);
		new db6(v);
		}
}
