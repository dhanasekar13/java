package inner_classes;

public class method_local {

	public void m1()
	{
		class inner
		{
			public void m2()
			{
				System.out.println("mehtod local inner class");
			}			
		}
		inner i=new inner();
		i.m2();
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
new method_local().m1();
	}

}
