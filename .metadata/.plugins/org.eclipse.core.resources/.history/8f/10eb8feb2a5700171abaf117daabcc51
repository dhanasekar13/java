public class remove_duplicate_element_in_String {
	static void set() {
		String a = "dhanasekard", b="";
		LinkedHashSet<Character> h = new LinkedHashSet<Character>();
		for (int i = 0; i < a.length(); i++) {
			h.add(a.charAt(i));
		}
		ArrayList<Character> hi=new ArrayList<Character>(h);hi.addAll(h);
		for (int j = 0; j < hi.size(); j++)
		{
				b=b+hi.get(j);
		}
		System.out.println(b);
	}
	
	
