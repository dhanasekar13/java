package task1;

import java.util.ArrayList;

public class db {
	ArrayList<Integer> phoneno=new ArrayList<>();
	ArrayList<String> name=new ArrayList<>();
	ArrayList<String> emailid=new ArrayList<>();

	db(int ph,String nam,String mail)
	{
		phoneno.add(ph);
		name.add(nam);
		emailid.add(mail);
	}
	db()
	{
		phoneno.get(0);
		name.get(0);
		emailid.get(0);
	}

}
