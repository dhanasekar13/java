package task;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayList;
import java.util.LinkedHashSet;
import java.util.Scanner;

public class remove_duplicate_element_in_String {
	static void set() {
		String a = "dhanasekard", b="";
		LinkedHashSet<Character> h = new LinkedHashSet<Character>();h.clear();
		for (int i = 0; i < a.length(); i++) {
			h.add(a.charAt(i));
		}

		ArrayList<Character> hi=new ArrayList<Character>(h);
		hi.clear();
		hi.addAll(h);
		for (int j = 0; j < hi.size(); j++)
		{
				b=b+hi.get(j);
		}
		System.out.println(b);
	}
	
	
	public static void main(String[] args) throws IOException {
	//	set();
		BufferedReader s = new BufferedReader(new InputStreamReader(System.in));
		
			ArrayList<ArrayList> a3=new ArrayList<ArrayList>();
		System.out.println("how many data are you need to enter into the array list 1");
		int q=s.read();
		ArrayList[] list=new ArrayList[q];
		for(int i=0;i<=q;i++)
		{
			System.out.println("enter the name");
			String q1=s.readLine();
			list[i].add(q1);
			System.out.println("enter the id");
			String q2=s.readLine();
			list[i].add(q1);
			a3.add(list[i]);
		}
		}
	}
