package collection;
//array list concept
import java.util.ArrayList;
import java.util.*;

class arrayl{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ArrayList a=new ArrayList();
		a.add("dhansekar");
		a.add('s');
		a.add(14);
		a.add(5.3);a.add('s');
		a.add(6.4f);
		a.add("6.4f");a.add('s');
		a.add("dhansekar");a.add('s');
		a.add(null);
		a.remove('s');
		for(int y=0;y<a.size();y++)
		{
			System.out.println(a.size());
		}	
	}

}
