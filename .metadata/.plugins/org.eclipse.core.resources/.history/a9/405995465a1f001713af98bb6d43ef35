package task;

 public class clone {
int a;
int b;

	public static void main(String[] args) throws CloneNotSupportedException {
		// TODO Auto-generated method stub
		clone i=new clone();
		i.a=2;i.b=4;
		System.out.println(i.a+" "+i.b);
		i.a=4;i.b=5;
		System.out.println(i.a+" "+i.b);
		clone j=(clone)i.clone();
		System.out.println(j.a+" "+j.b);
		i.a=0;i.b=0;
		System.out.println(i.a+" "+i.b);
		System.out.println(j.a+" "+j.b);

	}

}
