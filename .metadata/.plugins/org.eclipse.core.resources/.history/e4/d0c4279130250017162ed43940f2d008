package task1;

import java.util.ArrayList;

public class db {
	ArrayList<Long> phoneno=new ArrayList<>();
	ArrayList<String> name=new ArrayList<>();
	ArrayList<String> emailid=new ArrayList<>();

	db(long ph,String nam,String mail)
	{
		phoneno.add(ph);
		name.add(nam);
		emailid.add(mail);
	}
	db()
	{
		System.out.println(phoneno.get(0));
		System.out.println(name.get(0));
		System.out.println(emailid.get(0));
	}

}
