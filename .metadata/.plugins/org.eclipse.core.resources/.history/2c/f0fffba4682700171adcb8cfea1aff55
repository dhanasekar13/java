class db1 {
	static TreeSet<String> phoneno=new TreeSet<>();
	Scanner na=new Scanner(System.in);
	db1()
	{
		System.out.println("Enter the Phone number-");
		String mam=na.nextLine();
			phoneno.add(mam);
			}
	db1(int a)
	{
		System.out.println(phoneno);
	}
	}
	
