package task;

import java.util.Scanner;

public class duplicate {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner s=new Scanner(System.in);
System.out.println("enter the string ");
String r=s.nextLine();
int hi=0;
int ci=r.length();
char[] d=new char[ci];
char[] du=new char[ci];
int g=0,q=0;
    for (int i=0;i<ci;i++)
{
	d[i]=r.charAt(i);
	
}
    
    du[0]=d[0];
      for(int i=0;i<ci;i++)
        {
    	  
    	  
    	  for(int j=1;j<ci;j++)
    	  {  
    		 
                 if(d[i]==d[j])
                 {
                	hi++; 
                	du[g]=d[i];
                	g++;
                
                 }
                 else
                 {
                	
                 }
    	 
          }
    	
    	  System.out.println("string element "+d[i]+"  has "+hi+"  of repeating element  ");
    	  hi=0;
}
}
}